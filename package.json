{
  "name": "expo-game-support",
  "version": "0.0.20",
  "description": "Game support library for Expo/React Native with physics, robust game loop, and optimized touch input.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "expo",
    "react-native",
    "game",
    "physics",
    "game-loop",
    "touch-input",
    "mobile-games"
  ],
  "author": "Cesar Andres Pereira Bernal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesardev31/expo-game-support.git"
  },
  "bugs": {
    "url": "https://github.com/cesardev31/expo-game-support/issues"
  },
  "homepage": "https://github.com/cesardev31/expo-game-support#readme",
  "peerDependencies": {
    "expo": "^53.0.22",
    "react": "^19.1.1",
    "react-native": "^0.81.1",
    "expo-asset": "^11.1.7",
    "expo-av": "^15.1.7",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^3.19.1",
    "expo-gl": "^13.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.3",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^3.19.1",
    "expo-gl": "^13.6.0"
  },
  "dependencies": {}
}
