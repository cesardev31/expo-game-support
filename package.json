    "react-native",
    "game",
    "physics",
    "game-loop",
    "touch-input",
    "mobile-games"
  ],
  "author": "Cesar Andres Pereira Bernal",
  "license": "MIT",
  "peerDependencies": {
    "expo": ">=53.0.0",
    "react": ">=19.0.0",
    "react-native": ">=0.72.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "react-native-gesture-handler": "^2.12.0",
    "react-native-reanimated": "^3.5.0"
  }
